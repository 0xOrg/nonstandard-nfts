{
  "name": "@amxx/subgraphs-compound-governance",
  "version": "1.0.0",
  "description": "",
  "author": "Hadrien Croubois (@Amxx)",
  "license": "MIT",
  "homepage": "https://github.com/amxx/subgraphs.git#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amxx/subgraphs.git"
  },
  "bugs": {
    "url": "https://github.com/amxx/subgraphs.git/issues"
  },
  "scripts": {
    "prepublish":        "rimraf artifacts build cache generated",
    "prepare":           "yarn prepare:contracts && yarn prepare:schema",
    "prepare:contracts": "hardhat compile",
    "prepare:schema":    "mkdir -p generated && concat -o generated/schema.graphql src/schema.gql node_modules/@amxx/graphprotocol-utils/schema.graphql",
    "setup":             "templates/generator.js",
    "build:codegen":     "graph codegen ${SUBGRAPH:-generated/subgraph.yaml}",
    "build:compile":     "graph build   ${SUBGRAPH:-generated/subgraph.yaml}",
    "build:all":         "yarn build:codegen && yarn build:compile",
    "deploy-internal":   "yarn build:all && graph deploy --debug --node ${GRAPHNODE:-https://api.thegraph.com/deploy/} --ipfs ${IPFSNODE:-https://api.thegraph.com/ipfs/} ${NAME:-amxx/sandbox} ${SUBGRAPH:-generated/subgraph.yaml}",
    "deploy:mainnet":    "NAME=amxx/compound-governance        SUBGRAPH=subgraph.mainnet.yaml   yarn deploy-internal",
    "deploy:goerli":     "NAME=amxx/compound-governance-goerli SUBGRAPH=subgraph.goerli.yaml    yarn deploy-internal",
    "deploy:kovan":      "NAME=amxx/compound-governance-kovan  SUBGRAPH=subgraph.kovan.yaml     yarn deploy-internal",
    "deploy:sandbox":    "NAME=amxx/sandbox                    SUBGRAPH=generated/subgraph.yaml yarn deploy-internal"
  },
  "devDependencies": {
    "@amxx/graphprotocol-utils": "^0.0.8",
    "@graphprotocol/graph-cli": "^0.19.0",
    "@graphprotocol/graph-ts": "^0.19.0",
    "concat": "^1.0.3",
    "hardhat": "^2.1.2",
    "prompts": "^2.4.1",
    "rimraf": "^3.0.2"
  }
}
